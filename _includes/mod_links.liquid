{% if page.curse_project %}
    <li class="cf-link toc-external-link">
        <a href="https://www.curseforge.com/minecraft/mc-mods/{{ page.slug }}" title="CurseForge">
            {% include svg/cf-logo.svg %}
            {% include svg/external-link.svg %}
        </a>
    </li>
{% endif %}
{% if page.modrinth %}
    <li class="mr-link toc-external-link">
        <a href="https://www.modrinth.com/mod/{{ page.slug }}" title="modrinth">
            {% include svg/modrinth-logo.svg %}
            {% include svg/external-link.svg %}
        </a>
    </li>
{% endif %}
<li class="gh-link toc-external-link">
    <a href="https://www.github.com/Ladysnake/{{ page.slug }}" title="GitHub">
        <img src="/img/GitHubWord_0.png" alt="GitHub"/>
        {% include svg/external-link.svg %}
    </a>
</li>
{% if page.extra_toc_links %}
    {% include {{page.extra_toc_links}} %}
{% endif %}
